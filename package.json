{
  "name": "title-case-it",
  "version": "1.0.1",
  "description": "Title-case your headings to make them stand out.",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "repository": "6f5/title-case-it",
  "scripts": {
    "test": "test",
    "prepublish": "npm run build",
    "build": "rollup -c",
    "build:w": "rollup -c -w"
  },
  "files": [
    "dist/*"
  ],
  "keywords": [
    "title case",
    "to title case",
    "title",
    "case",
    "article",
    "blog",
    "seo"
  ],
  "author": "Abdulla Saeed <hi@restoredbytes.com>",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-babel": "6.0.4",
    "@rollup/plugin-commonjs": "26.0.1",
    "@rollup/plugin-node-resolve": "15.2.3",
    "rollup": "^4.18.0"
  }
}
